{
  "expo": {
    "name": "Dev Location",
    "slug": "DevLocation",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8b54a8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leoronne.devlocation"
    },
    "android": {
      "package": "com.leoronne.devlocation",
      "icon": "./assets/img/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logoTransparent.png",
        "backgroundColor": "#8b54a8"
      }
    }
  }
}
